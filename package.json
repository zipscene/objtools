{
  "name": "zs-objtools",
  "version": "0.3.1",
  "description": "Various utility functions for working with object, including object merging, inheritance, deep copying, etc.",
  "main": "./dist/lib/index.js",
  "repository": {
    "type": "git",
    "url": "git@git.zipscene.com:zsapilibs/zs-objtools.git"
  },
  "scripts": {
    "lint": "node ./node_modules/.bin/eslint . || exit 0",
    "docs": "node ./node_modules/.bin/yuidoc .",
    "test": "npm run-script build && node ./node_modules/.bin/mocha ./dist/test -R spec",
    "build": "./node_modules/.bin/grunt build && rm -rf tmp",
    "watch": "./node_modules/.bin/grunt watch && rm -rf tmp",
    "prepublish": "npm run-script build && npm run-script docs"
  },
  "dependencies": {
    "babel-runtime": "^4.7.12",
    "xerror": "^1.0.0"
  },
  "devDependencies": {
    "broccoli": "^0.15.0",
    "broccoli-babel-transpiler": "^4.0.1",
    "broccoli-cli": "0.0.1",
    "broccoli-merge-trees": "^0.2.1",
    "broccoli-static-compiler": "^0.2.1",
    "chai": "^2.1.1",
    "es6-promise": "^2.0.1",
    "eslint": "^0.17.0",
    "grunt": "^0.4.5",
    "grunt-broccoli": "^0.4.0",
    "grunt-cli": "^0.1.13",
    "mocha": "^2.2.1",
    "yuidocjs": "^0.6.0"
  },
  "publishConfig": {
    "registry": "https://npm.zipscene.com/"
  }
}
